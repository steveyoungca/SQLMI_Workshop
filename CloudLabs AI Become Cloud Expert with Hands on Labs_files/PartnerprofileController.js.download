angular.module('cloudsandbox.studentportal').controller('PartnerprofileController', ['$rootScope', '$scope', '$http', 'appConstant', 'serviceEndpoint', 'adalAuthenticationService', function ($rootScope, $scope, $http, appConstant, serviceEndpoint, adalService) {
    $scope.$on('$viewContentLoaded', function () {
        var vm = this;

        //get all partner details 
        $http.get(appConstant.apiRoot + serviceEndpoint.partner).
                 then(function successCallback(response) {
                     $scope.partnerprofile = response.data;
                 });
    });
}]);
